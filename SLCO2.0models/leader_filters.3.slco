						turn[curr] = 2
					}

					from p8 to p9 {
						curr > 0 and c[curr-1] = 0
					}

					from p9 to elected {
						
					}

					from p8 to p1 {
						[curr = 0 or c[curr-1] = 1; curr := curr+1]
					}

				}
	
			P_3 {
				variables
