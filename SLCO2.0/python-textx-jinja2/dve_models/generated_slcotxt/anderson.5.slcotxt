model dve_anderson_5 {
classes
	GlobalClass {
		variables
			Integer[5] Slot := [1,0,0,0,0,0]
			Integer next := 0
		state machines
			P_0 {
				variables
					Integer myplace
				initial NCS
				states  p1 p2 p3 CS
				transitions
					from NCS to p1 {
						myplace := next;next := next+1
					}

					from p1 to p2 {
						myplace = 5-1; next := next-5
					}

					from p1 to p2 {
						myplace <> 5-1; myplace := myplace%5
					}

					from p2 to p3 {
						Slot[myplace] = 1
					}

					from p3 to CS {
						Slot[(myplace+5-1)%5] := 0
					}

					from CS to NCS {
						Slot[(myplace+1)%5] := 1
					}

				}
	
